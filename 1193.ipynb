   "source": [
    "x = int(input())\n",
    "\n",
    "n = 1\n",
    "while( x > n) :\n",
    "    x = x - n\n",
    "    n = n + 1\n",
    "if n % 2 == 0 :\n",
    "    print(\"%d/%d\"%(x,n-x+1))\n",
    "else :\n",
    "    print(\"%d/%d\"%(n-x+1,x))"
   ]
