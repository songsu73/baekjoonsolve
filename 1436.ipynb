    "k = 1\n",
    "i = 666\n",
    "while(True) :\n",
    "    if (n == k) :\n",
    "        break    \n",
    "    i += 1\n",
    "    j = str(i)\n",
    "    for w in range(len(j),0,-1) :\n",
    "        if j[w-3:w] == '666' :\n",
    "            k += 1\n",
    "            break\n",
    "\n",
    "print(i)"
