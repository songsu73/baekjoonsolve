    "result = 0\n",
    "\n",
    "while(n > 0) :\n",
    "    if n % 5 == 0 :\n",
    "        n -= 5\n",
    "        result += 1\n",
    "    elif n % 3 == 0 :\n",
    "        n -= 3\n",
    "        result += 1\n",
    "    elif n > 5 :\n",
    "        n -= 5\n",
    "        result += 1\n",
    "    \n",
    "print(result)"
