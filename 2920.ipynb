    "inp = input()\n",
    "inp = inp.split()\n",
    "inp = list(map(int,inp))\n",
    "\n",
    "sortlist = sorted(inp)\n",
    "desortlist = list(reversed(sorted(inp)))\n",
    "\n",
    "if sortlist == inp :\n",
    "    print(\"ascending\")\n",
    "elif desortlist == inp :\n",
    "    print(\"descending\")\n",
    "else :\n",
    "    print(\"mixed\")"
   ]
